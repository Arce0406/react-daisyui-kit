{
  "name": "react-daisyui-kit",
  "version": "1.0.0",
  "private": false,
  "description": "Monorepo for react-daisyui-kit",
  "scripts": {
    "check-types": "tsc --noEmit",
    "kit:build": "pnpm --filter @featherstudio/react-daisyui-kit run build",
    "kit:publish": "pnpm run kit:build && cd packages/react-daisyui-kit && pnpm publish",
    "demo:build": "pnpm run kit:build && cd websites/demo-client && pnpm run build",
    "dev": "pnpm --filter demo-client run dev",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "pnpm run kit:build && changeset publish"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.30.1",
  "workspaces": [
    "packages/*",
    "websites/*"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.29.8"
  }
}
