{
  "name": "react-daisyui-kit",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for react-daisyui-kit",
  "scripts": {
    "check-types": "tsc --noEmit",
    "build:lib": "pnpm --filter @featherstudio/react-daisyui-kit run build",
    "dev": "pnpm --filter demo-client run dev",
    "publish": "pnpm run build:lib && cd packages/react-daisyui-kit && pnpm publish"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "workspaces": [
    "packages/*",
    "websites/*"
  ]
}
